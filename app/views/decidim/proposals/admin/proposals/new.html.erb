<% unless @form.component.settings.participatory_texts_enabled? %>
	<%= stylesheet_link_tag    "oh/admin/proposals", media: "all" %>
	<%= javascript_include_tag "oh/admin/proposals" %>
<% end %>

<%= decidim_form_for(@form, html: { class: "form new_proposal proposal_form_admin" }) do |f| %>
  <%= render partial: "form", object: f, locals: { title: t(".title") } %>

  <div class="button--double form-general-submit">
    <%= f.submit t(".create") %>
  </div>
<% end %>
